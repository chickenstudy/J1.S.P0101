    <servlet>
        <servlet-name>DetailCourse</servlet-name>
        <servlet-class>controller.DetailCourseController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateQuestionController</servlet-name>
        <servlet-class>controller.CreateQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteQuestionController</servlet-name>
        <servlet-class>controller.DeleteQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateQuestionController</servlet-name>
        <servlet-class>controller.UpdateQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTestController</servlet-name>
        <servlet-class>controller.CreateTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTestController</servlet-name>
        <servlet-class>controller.DeleteTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailTestController</servlet-name>
        <servlet-class>controller.DetailTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateTestController</servlet-name>
        <servlet-class>controller.UpdateTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateQuestionTestCotroller</servlet-name>
